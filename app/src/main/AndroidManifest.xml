<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xavier.rxexample">

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:theme="@style/AppTheme">

    <activity
        android:name=".ImplementationSelectorActivity"
        android:label="@string/app_name"
        >

      <intent-filter>

        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>

      </intent-filter>

    </activity>

    <activity
        android:name=".MeloticMarketListActivity"
        android:label="@string/title_market_list"
        />

    <activity
        android:name=".MeloticMarketActivity"
        android:label="@string/title_market_depth"
        />

    <activity
        android:name=".MeloticMarketListRxOkActivity"
        android:label="@string/title_market_list_rx_ok"
        />

    <activity
        android:name=".MeloticMarketListRxFailActivity"
        android:label="@string/title_market_list_rx_fail"
        />

    <activity
        android:name=".BterTradingPairListActivity"
        android:label="@string/title_bter_trading_pairs"
        />

    <activity
        android:name=".BterTradingPairListRxActivity"
        android:label="@string/title_bter_trading_pairs_rx"
        />

  </application>

</manifest>
